<section style="padding-top:150px;">
    <div class="container">
        <div class="row">   

        <table class="table">
        <thead>
            <tr>
            <th scope="col">No</th>
            <th scope="col">Id</th>
            <th scope="col">UserName</th>
            <th scope="col">Email Id</th>
            </tr>
        </thead>
        <tbody>
            {{#each users}}
            <tr>
            <th scope="row" class="create_index">{{@index}}</th>
            <td>{{this._id}}</td>
            <td>{{this.Username}}</td>
            <td>{{this.Email}}</td>
            </tr>
            {{/each}}

            
        </tbody>
        </table>
        </div>
    </div>   
</section>
