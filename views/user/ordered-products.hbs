<section>
    <div class="container">
        <div class="row " style="padding-top: 130px;">
             {{#each products}}
        <div class="col-sm-6 col-md-4 col-lg-3 p-3">
            <div class="card" style="width: 15rem; height:30rem;">
            <img class="card-img-top" src="/product-images/{{this.products._id}}.jpg" alt="Card image cap">
            <div class="card-body">
                <h5 class="card-title">{{this.products.Name}}</h5>
                <p class="card-text">Rs.{{this.products.Price}}</p>
            
            </div>
            </div>
        </div>
        {{/each}}
        </div>

    </div>
</section>